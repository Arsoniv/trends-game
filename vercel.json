{
    "version": 2,
    "builds": [
      { "src": "api/**/*.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1.js" },
      { "src": "/(.*)", "dest": "/public/home/$1" },
      { "src": "/host/(.*)", "dest": "/public/host/$1" },
      { "src": "/host/(.*)", "dest": "/public/play/$1" }
    ]
  }
  